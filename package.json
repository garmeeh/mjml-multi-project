{
  "name": "mjml-multi-project",
  "version": "1.0.0",
  "description": "Email template workflow with MJML",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/garmeeh/mjml-multi-project.git"
  },
  "scripts": {
    "compile": "node ./scripts/index.js --compile",
    "clean-up": "node ./scripts/index.js --clean",
    "create-project": "node ./scripts/index.js --createProject",
    "compile-all": "node ./scripts/index.js --compileAll",
    "watch": "node ./scripts/index.js --watch",
    "contributors:add": "all-contributors add",
    "contributors:generate": "all-contributors generate"
  },
  "lint-staged": {
    "*.js": [
      "prettier --single-quote --write",
      "eslint",
      "git add",
      "jest --bail --findRelatedTests"
    ]
  },
  "author": "Gary Meehan",
  "license": "MIT",
  "homepage": "https://github.com/garmeeh/mjml-multi-project#readme",
  "engines": {
    "node": "^8.5.0",
    "yarn": "^1.3.2"
  },
  "prettier": {
    "singleQuote": true
  },
  "devDependencies": {
    "all-contributors-cli": "^4.11.1",
    "babel-core": "^6.26.0",
    "babel-jest": "^22.2.2",
    "babel-preset-env": "^1.6.1",
    "chalk": "^2.3.1",
    "clear": "^0.1.0",
    "commander": "^2.14.1",
    "eslint": "^4.18.2",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "figlet": "^1.2.0",
    "glob": "^7.1.2",
    "husky": "^0.14.3",
    "inquirer": "^5.1.0",
    "jest": "^22.3.0",
    "lint-staged": "^7.0.0",
    "mjml": "^4.0.0",
    "node-fetch": "^2.0.0",
    "node-watch": "^0.5.7",
    "prettier": "^1.11.1",
    "regenerator-runtime": "^0.11.1"
  },
  "dependencies": {
    "mkdirp": "^0.5.1",
    "rimraf": "^2.6.2"
  }
}
